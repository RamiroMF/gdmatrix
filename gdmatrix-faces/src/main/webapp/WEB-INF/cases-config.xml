      <to-view-id>/common/cases/case_search.jsp</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>case_main</from-outcome>
      <to-view-id>/common/cases/case_main.jsp</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-outcome>case_detail</from-outcome>
