   <renderer-class>org.santfeliu.faces.fckeditor.renderkit.html.CKEditorRenderer</renderer-class>
  </renderer>
  <renderer>
   <component-family>javax.faces.Output</component-family>
   <renderer-type>javax.faces.Text</renderer-type>
   <renderer-class>org.santfeliu.faces.render.HtmlOutputTextRenderer</renderer-class>
  </renderer>
  <renderer>
   <component-family>javax.faces.Output</component-family>
   <renderer-type>javax.faces.RichText</renderer-type>
   <renderer-class>org.santfeliu.faces.render.HtmlOutputRichTextRenderer</renderer-class>
  </renderer>
  <renderer>
   <component-family>DynamicForm</component-family>
